<div class="col-xs-10 col-xs-offset-1">
  <ul class="breadcrumb">
    <% @breadcrumbs.each { |bread| %>
        <% if bread[:class] == 'active' %>
          <li class="<%= bread[:class] %>">
            <%= bread[:text] %>
          </li>
        <% else %>
          <li class="<%= bread[:class] %>">
            <a href="<%= bread[:href] %>"><%= bread[:text] %></a>
          </li>
        <% end %>
    <% } %>
  </ul>

  <div class="panel panel-default">
    <div class="panel-body college-data">
      <pre>
        <%= @institute.to_json %>
      </pre>
    </div>
  </div>
</div>
